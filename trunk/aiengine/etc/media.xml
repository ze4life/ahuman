<service>

	<listeners>
		<listener name="debug" run="true">
			<property name="direction" value="duplex"/>
			<property name="auth" value="false"/>
			<property name="port" value="20001"/> 
			<property name="msgtype" value="xmlcall"/> 
			<property name="topic.in" value="debug.commands.in"/> 
			<property name="topic.out" value="debug.commands.out"/> 
		</listener>
	</listeners>

	<directchannels>
		<channel name="google" run="true">
			<property name="type" value="request-response"/>
			<property name="permanent" value="false"/>
			<property name="direction" value="duplex"/>
			<property name="host" value="20001"/> 
			<property name="port" value="20001"/> 
			<property name="msgtype" value="text"/> 
			<property name="topic.in" value="debug.commands.in"/> 
			<property name="topic.out" value="debug.commands.out"/> 
		</channel>

		<channel name="image-processor" run="false">
			<property name="type" value="request-response"/>
			<property name="permanent" value="false"/>
			<property name="direction" value="duplex"/>
			<property name="host" value="localhost"/> 
			<property name="port" value="9000"/> 
			<property name="msgtype" value="text"/> 
			<property name="topic.in" value="debug.commands.in"/> 
			<property name="topic.out" value="debug.commands.out"/> 
		</channel>
	</directchannels>

</service>

<service>
	<MindMap>
		<MindArea id="Sensors" size="10000">
			<MindLink masterArea="Sensors" slaveArea="PrimaryCortex"/>
		</MindArea>
		<MindArea id="PrimaryCortex" size="100000">
			<MindLink masterArea="PrimaryCortex" slaveArea="EnthorinalCortex"/>
		</MindArea>
		<MindArea id="EnthorinalCortex" size="100000">
		</MindArea>
	</MindMap>
	<ActiveMemory>
		<MemoryFocus>
			<property name="objectCount" value="100"/>
		</MemoryFocus>
		<ThreadPool>
			<property name="threadCount" value="100"/>
			<property name="dynamicLoad" value="true"/>
			<property name="reportGroup" value="10"/>
			<property name="minLoad" value="10"/>
			<property name="maxLoad" value="30"/>
		</ThreadPool>
	</ActiveMemory>
</service>

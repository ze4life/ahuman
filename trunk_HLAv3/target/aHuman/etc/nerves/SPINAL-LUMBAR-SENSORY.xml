<pns>

	<division category="Spinal Nerves" name="LUMBAR NERVES - SENSORY">

		<element name="fibular nerves"
			modality="general sensory"
			>
			<element name="musculocutaneous nerve of foot"
				origin="common fibular nerve"
				synonyms="MA:[http://www.oluwoleogunranti.com/course/course1/nerves/Ne94.htm SUPERFICIAL FIBULAR NERVE], superficial fibular nerve"
				branches="medial - medial dorsal cutaneous nerve, lasteral -  intermediate dorsal cutaneous nerve"
				modality="general sensory"
				>
				<element name="cutaneous nerve of foot, medial dorsal"
					origin="superficial fibular nerve"
					branches="dorsal digital nerves of foot"
					distribution="nerves supplying 3-5 toes, skin and joints of medial side of foot and 1 toe, and adjacent sides of 2/3 toes; nerves supplying 1/2 toes, skin and joints of adjacent sides of 1/2 toes"
					modality="general sensory"
					>
					<element name="sural nerve"
						synonyms="MA:[http://www.oluwoleogunranti.com/course/course1/nerves/Ne87.htm SURAL NERVE]"
						origin="tibial nerve"
						branches="lateral dorsal cutaneous nerve and lateral calcaneal branches"
						distribution="skin on back of leg, and skin and joints on lateral side of heel and foot"
						modality="general sensory"
						>
					</element>
				</element>
				<element name="cutaneous nerve of foot, intermediate dorsal"
					origin="superficial fibular nerve"
					branches="dorsal digital nerves of foot"
					distribution="skin of front of lower third of leg and dorsum of foot, and skin and joints of adjacent sides of 3/4 toes, and of 4/5 toes"
					modality="general sensory"
					>
					<element name="digital nerves of foot, dorsal"
						origin="intermediate dorsal cutaneous nerve"
						distribution="skin and joints of adjacent sides of 3/4 toes, and of 4/5 toes"
						synonyms="MA:[http://www.oluwoleogunranti.com/course/course1/nerves/Ne213.htm DEEP FIBULAR NERVE], deep fibular nerve"
						modality="general sensory"
						>
						<element name="cutaneous nerve of foot, lateral dorsal"
							origin="continuation of sural nerve"
							distribution="skin and joints of lateral side of foot and 5 toe"
							modality="general sensory"
						/>
					</element>
				</element>
			</element>
		</element>

		<element name="perforating cutaneous nerve"
			synonyms="MA:[http://www.oluwoleogunranti.com/course/course1/nerves/Ne95.htm PERFORATING CUTANEOUS NERVE]"
			origin="one of the inferior cluneal nerves which pierces the sacrotuberous ligament and supplies the skin over the inferomedial gluteus maximus; it is absent in one third of the population"
			modality="general sensory"
			/>

		<element name="cutaneous nerve of thigh"
			<element name="cutaneous nerve of thigh, lateral"
				synonyms="MA:[http://www.oluwoleogunranti.com/course/course1/nerves/Ne72.htm LATERAL CUTANEOUS NERVE OF THIGH]"
				origin="lumbar PLEXUS L2-L3" 
				distribution="skin of lateral and frontal aspects of thigh"
				modality="general sensory"
				/>
		</element>

		<element name="ilioinguinal nerve"
			synonyms="MA:[http://www.oluwoleogunranti.com/course/course1/nerves/Ne41.htm ILIOINGUINAL NERVE]"
			origin="lumbar PLEXUS L1 (sometimes T12); accompanies the spermatic cord through the inguinal canal"
			branches="anterior scrotal or labial branches"
			distribution="skin of scrotum or labia majora, and adjacent part of thigh"
			modality="general sensory"
			>
			<element name="labial nerves, anterior"
				origin="ilioinguinal nerve"
				distribution="skin of anterior labial region of labia majora, and adjacent part of thigh"
				modality="general sensory"
				/>
			<element name="scrotal nerves, anterior"
				origin="ilioinguinal nerve"
				distribution="skin of anterior scrotal region"
				modality="general sensory"
				/>
		</element>

	</division>

</pns>


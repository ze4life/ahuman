<pns>

	<division category="Spinal Nerves" name="THORACIC NERVES - MOTOR ROOTS">

		<element name="01, T1 nerve"
			synonyms="MA:[http://www.oluwoleogunranti.com/course/course1/nerves/ne26.htm T1 NERVE]"
			imgsrc="http://dermatologic.com.ar/c1tt2.png"
			modality="flexor motor"
			>
			<element name="T1 nerve, large branch"
				modality="flexor motor"
				action="upper limb fibers"
				>
				<fibers type="GSE" src="BMA.SC.VH.PM" dst="TARGET.TEA.HAND"></fibers>
				<fibers type="GSE" src="BMA.SC.IZ.RPL" dst="TARGET.TEA.HAND"></fibers>
			</element>
			<element name="T1 nerve, first intercostal nerve"
				synonyms="small branch"
				distribution="1st rib"
				modality="flexor motor"
				>
				<fibers type="GSE" src="BMA.SC.VH.PM" dst="TARGET.TEA.TRUNK"></fibers>
			</element>
		</element>

		<element name="02-06, upper thoracic nerves"
			synonyms="thoracic intercostal nerves"
			imgsrc="http://www.rci.rutgers.edu/~uzwiak/AnatPhys/APFallLect10_files/image013.jpg"
			modality="flexor motor, extensor motor"
			action="parietes of the thorax; trunk flexor (rectus abdominis, intercostals)"
			>
			<fibers type="GSE" src="BMA.SC.VH.PM" dst="TARGET.TEA.TRUNK"></fibers>
			<element name="T2 nerve"
				synonyms="MA:[http://www.oluwoleogunranti.com/course/course1/nerves/ne27.htm T2 NERVE]"
				modality="flexor motor"
				/>
			<element name="T3 nerve"
				synonyms="MA:[http://www.oluwoleogunranti.com/course/course1/nerves/ne28.htm T3 NERVE]"
				modality="flexor motor"
				/>
			<element name="T4 nerve"
				synonyms="MA:[http://www.oluwoleogunranti.com/course/course1/nerves/ne29.htm T4 NERVE]"
				modality="flexor motor"
				/>
			<element name="T5 nerve"
				synonyms="MA:[http://www.oluwoleogunranti.com/course/course1/nerves/ne30.htm T5 NERVE]"
				modality="flexor motor"
				/>
			<element name="T6 nerve"
				synonyms="MA:[http://www.oluwoleogunranti.com/course/course1/nerves/ne31.htm T6 NERVE]"
				modality="flexor motor"
				/>
		</element>

		<element name="07-11, lower thoracic nerves"
			synonyms="thoracicoabdominal intercostal nerves"
			imgsrc="http://www.cixip.com/Public/kindeditor/attached/image/20121122/20121122104412_41232.jpg"
			distribution="Rectus abdominis; Intercostales; Serratus posterior inferior"
			modality="flexor motor"
			action="parietes of the thorax and abdomen; forced respiration"
			>
			<fibers type="GSE" src="BMA.SC.VH.PM" dst="TARGET.TEA.TRUNK"></fibers>
			<element name="T7 nerve"
				synonyms="MA:[http://www.oluwoleogunranti.com/course/course1/nerves/ne32.htm T7 NERVE]"
				distribution="supply muscles of abdomen - external oblique, internal oblique, transversus abdominis, rectus abdominis"
				imgsrc="http://www.cutandjacked.com/sites/default/files/images/abs/manage_abs5.jpg"
				modality="flexor motor"
				/>
			<element name="T8 nerve"
				synonyms="MA:[http://www.oluwoleogunranti.com/course/course1/nerves/ne33.htm T8 NERVE]"
				distribution="supply muscles of abdomen - external oblique, internal oblique, transversus abdominis, rectus abdominis"
				modality="flexor motor"
				/>
			<element name="T9 nerve"
				synonyms="MA:[http://www.oluwoleogunranti.com/course/course1/nerves/ne34.htm T9 NERVE]"
				distribution="supplies muscles of anterior abdominal wall - internal oblique, transversus abdominis, rectus abdominis"
				modality="flexor motor"
				/>
			<element name="T10 nerve"
				synonyms="MA:[http://www.oluwoleogunranti.com/course/course1/nerves/ne35.htm T10 NERVE]"
				distribution="supplies muscles of anterior abdominal wall - internal oblique, rectus abdominis, transversus abdominis"
				modality="flexor motor"
				/>
			<element name="T11 nerve"
				synonyms="MA:[http://www.oluwoleogunranti.com/course/course1/nerves/ne36.htm T11 NERVE]"
				distribution="supplies muscles of anterior abdominal wall - internal oblique, rectus abdominis, transversus abdominis"
				modality="flexor motor"
				/>
		</element>

		<element name="12, T12 nerve"
			synonyms="MA:[http://www.oluwoleogunranti.com/course/course1/nerves/ne37.htm 12TH THORACIC NERVE]"
			imgsrc="http://painmuse.org/images/pyramidalis.jpg"
			modality="flexor motor"
			action="internal oblique muscle and transversus abdominis; forced respiration"
			>
			<fibers type="GSE" src="BMA.SC.VH.PM" dst="TARGET.TEA.TRUNK"></fibers>
			<element name="subcostal nerve"
				synonyms="MA:[http://www.oluwoleogunranti.com/course/course1/nerves/ne219.htm SUBCOSTAL NERVE]"
				distribution="supplies muscles - internal oblique, transversus abdominis; gives branch to pyramidalis"
				modality="flexor motor"
				/>
		</element>

	</division>

</pns>

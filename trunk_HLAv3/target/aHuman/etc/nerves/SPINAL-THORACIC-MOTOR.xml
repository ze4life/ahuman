<pns>

	<division category="Spinal Nerves" name="THORACIC NERVES - MOTOR">

		<element name="T1 nerve, motor"
			synonyms="MA:[http://www.oluwoleogunranti.com/course/course1/nerves/ne26.htm T1 NERVE]"
			imgsrc="http://dermatologic.com.ar/c1tt2.png"
			modality="flexor motor"
			>
			<element name="T1 nerve, large branch"
				modality="flexor motor"
				action="upper limb fibers"
				>
				<fibers type="GSE" src="BMA.SC.VH.PM" dst="TARGET.TEA.HAND"></fibers>
				<fibers type="GSE" src="BMA.SC.IZ.RPL" dst="TARGET.TEA.HAND"></fibers>
			</element>
			<element name="first intercostal nerve"
				synonyms="small branch"
				distribution="1st rib"
				modality="flexor motor"
				>
				<fibers type="GSE" src="BMA.SC.VH.PM" dst="TARGET.TEA.TRUNK"></fibers>
			</element>
		</element>

		<element name="thoracic intercostal nerves"
			imgsrc="http://www.rci.rutgers.edu/~uzwiak/AnatPhys/APFallLect10_files/image013.jpg"
			modality="flexor motor, extensor motor"
			action="parietes of the thorax; trunk flexor (rectus abdominis, intercostals)"
			>
			<fibers type="GSE" src="BMA.SC.VH.PM" dst="TARGET.TEA.TRUNK"></fibers>
		</element>

		<element name="thoracicoabdominal intercostal nerves"
			imgsrc="http://www.cixip.com/Public/kindeditor/attached/image/20121122/20121122104412_41232.jpg"
			distribution="Rectus abdominis; Intercostales; Serratus posterior inferior"
			modality="flexor motor"
			action="parietes of the thorax and abdomen; forced respiration"
			>
			<fibers type="GSE" src="BMA.SC.VH.PM" dst="TARGET.TEA.TRUNK"></fibers>
		</element>

		<element name="subcostal nerve"
			synonyms="MA:[http://www.oluwoleogunranti.com/course/course1/nerves/ne219.htm SUBCOSTAL NERVE]"
			imgsrc="http://painmuse.org/images/pyramidalis.jpg"
			modality="flexor motor"
			action="internal oblique muscle and transversus abdominis; forced respiration"
			>
			<fibers type="GSE" src="BMA.SC.VH.PM" dst="TARGET.TEA.TRUNK"></fibers>
		</element>

	</division>

</pns>

<service>

	<!-- ================= basic neural networks parameters =============== -->
	<neural-networks>
		<property name="NEURON_SYNAPTIC_THRESHOLD_INITIAL_pQ" value="70"/>
		<property name="NEURON_SYNAPTIC_THRESHOLD_MIN_pQ" value="10"/>
		<property name="NEURON_ACTION_POTENTIAL_BY_SIGNAL_pQ" value="20"/>
		<property name="NEURON_MEMBRANE_POTENTIAL_BY_ACTION_POTENTIAL_pQ" value="65"/>
		<property name="NEURON_MEMBRANE_THRESHOLD_INITIAL_pQ" value="60"/>
		<property name="NEURON_FIRE_OUTPUT_BY_MEMBRANE_POTENTIAL_pQ" value="80"/>
		<property name="NEURON_INHIBIT_DELAY_ms" value="10"/>
		<property name="NEURON_FIRE_OUTPUT_SILENT_ms" value="10"/>
		<property name="NEURON_FIRE_IMPULSE_pQ" value="2"/>
		<property name="NEURON_POTENTIAL_DISCHARGE_RATE_pQ_per_ms" value="1"/>
		<property name="NEURON_OUTPUT_DISCHARGE_RATE_pQ_per_ms" value="1"/>
		<property name="NEURON_FULL_RELAX_ms" value="1000"/>
		<property name="NEURON_CONNECTIVITY_UPDATE_FACTOR" value="95"/>
	</neural-networks>

	<!-- ================= mind geometry =============== -->
	<mind-space>
	</mind-space>

	<!-- ================= mind components and circuits =============== -->
	<mind-map>

		<region-type-set>

			<region-type name="nucleus" implementation="mock">
				<connector id="nucleus.input" type="target"/>
				<connector id="nucleus.output" type="source"/>
				<connector id="nucleus.interneurons" type="target"/>
			</region-type>

			<region-type name="neocortex" implementation="mock" notes="feed-forward and feed-back">
				<connector id="neocortex.ffin" type="target"/>
				<connector id="neocortex.ffout" type="source"/>
				<connector id="neocortex.fbin" type="target"/>
				<connector id="neocortex.fbout" type="source"/>
			</region-type>

			<region-type name="allocortex" implementation="mock" notes="feed-forward-only">
				<connector id="allocortex.input.direct" type="target" notes="polymorphic in HF/climbing fibers to molecular in CR"/>
				<connector id="allocortex.input.indirect" type="target" notes="molecular in HF/mossy fibers to granular in CR"/>
				<connector id="allocortex.output" type="source" notes="mossy fibers from polymorphic in HF/purkinje in CR"/>
			</region-type>

			<region-type name="ganglion" implementation="mock">
				<connector id="ganglion.input" type="target"/>
				<connector id="ganglion.output" type="source"/>
				<connector id="ganglion.feedback" type="target"/>
			</region-type>

		</region-type-set>

		<!-- ================= typical links =============== -->
		<connection-type-set>

			<connection-type name="nucleus-nucleus">
				<link-type name="positive" implementation="mock" type="excitatory" nt="GLU" src="nucleus.output" dst="nucleus.input"/>
			</connection-type>

			<connection-type name="nucleus-neocortex">
				<link-type name="primary-input" implementation="mock" type="excitatory" nt="GLU" src="nucleus.output" dst="neocortex.ffin"/>
			</connection-type>

			<connection-type name="neocortex-nucleus">
				<link-type name="primary-input" implementation="mock" type="excitatory" nt="GLU" src="neocortex.ffout" dst="nucleus.input"/>
			</connection-type>

			<connection-type name="neocortex-neocortex">
				<link-type name="cortex-forward" implementation="mock" type="excitatory" nt="GLU" src="neocortex.ffout" dst="neocortex.ffin"/>
				<link-type name="cortex-feedback" implementation="mock" type="excitatory" nt="GLU" src="neocortex.fbout" dst="neocortex.fbin" back="true"/>
			</connection-type>

			<connection-type name="neocortex-allocortex">
				<link-type name="light" implementation="mock" type="excitatory" nt="GLU" src="neocortex.ffout" dst="allocortex.input.indirect"/>
			</connection-type>

			<connection-type name="allocortex-neocortex">
				<link-type name="light" implementation="mock" type="excitatory" nt="GLU" src="allocortex.output" dst="neocortex.ffin"/>
			</connection-type>

			<connection-type name="allocortex-nucleus">
				<link-type name="light" implementation="mock" type="excitatory" nt="GLU" src="allocortex.output" dst="nucleus.input"/>
			</connection-type>

			<connection-type name="nucleus-ganglion">
				<link-type name="primary-input" implementation="mock" type="excitatory" nt="GLU" src="nucleus.output" dst="ganglion.input"/>
			</connection-type>

			<connection-type name="nucleus-allocortex">
				<link-type name="primary-input" implementation="mock" type="excitatory" nt="GLU" src="nucleus.output" dst="allocortex.input.direct"/>
			</connection-type>

			<connection-type name="allocortex-allocortex">
				<link-type name="light" implementation="mock" type="excitatory" nt="GLU" src="allocortex.output" dst="allocortex.input.direct"/>
			</connection-type>

		</connection-type-set>


<pns>

	<division category="MUSCLES - HEAD" name="Scalp/Eyelid" group="Facial muscles">

		<element name="Occipitofrontalis muscle">
			<element name="Frontalis muscle" type="cranial" link="http://en.wikipedia.org/wiki/Frontalis_muscle"
				nerves="facial nerve, temporal branch"
				action="raises the eyebrows"
				/>
			<element name="Occipitalis muscle" type="cranial" link="http://en.wikipedia.org/wiki/Occipitalis_muscle"
				nerves="auricular nerve, posterior, occipital branch"
				action="moves the scalp back"
				/>
		</element>

		<element name="Corrugator supercilii muscle" type="cranial" link="http://en.wikipedia.org/wiki/Corrugator_supercilii_muscle"
			nerves="facial nerve, temporal branch"
			action="wrinkles forehead"
			/>

		<element name="Depressor supercilii muscle" type="cranial" link="http://en.wikipedia.org/wiki/Depressor_supercilii_muscle"
			nerves="facial nerve, zygomatic branch"
			action="depression of eyebrow"
			/>

		<element name="Lacrimal gland" type="gland" link="http://en.wikipedia.org/wiki/Lacrimal_gland"
			nerves="petrosal nerve, greater, to lacrimal gland"
			action="secrete the aqueous layer of the tear film"
			/>

	</division>

	<division category="MUSCLES - HEAD" name="Extraocular muscles" group="Extraocular muscles">

		<element name="Levator palpebrae superioris muscle" type="cranial" link="http://en.wikipedia.org/wiki/Levator_palpebrae_superioris_muscle"
			nerves="oculomotor nerve"
			action="retracts/elevates eyelid"
			/>

		<element name="Superior tarsal muscle" type="cranial" link="http://en.wikipedia.org/wiki/Superior_tarsal_muscle"
			nerves="internal carotid nerve"
			action="raise the upper eyelid"
			/>

		<element name="Orbicularis oculi muscle">
			<element name="Orbicularis oculi muscle, temporal" type="cranial" link="http://en.wikipedia.org/wiki/Orbicularis_oculi_muscle"
				nerves="facial nerve, temporal branch"
				action="closes the eyelids"
				/>
			<element name="Orbicularis oculi muscle, zygomatic" type="cranial" link="http://en.wikipedia.org/wiki/Orbicularis_oculi_muscle"
				nerves="facial nerve, zygomatic branch"
				action="closes the eyelids"
				/>
		</element>

		<element name="Rectus">
			<element name="Inferior rectus muscle" type="cranial" link="http://en.wikipedia.org/wiki/Inferior_rectus_muscle"
				nerves="oculomotor nerve, inferior branch"
				action="depression and adduction"
				/>
			<element name="Lateral rectus muscle" type="cranial" link="http://en.wikipedia.org/wiki/Lateral_rectus_muscle"
				nerves="abducent nerve"
				action="abducts the eyeball"
				/>
			<element name="Medial rectus muscle" type="cranial" link="http://en.wikipedia.org/wiki/Medial_rectus_muscle"
				nerves="oculomotor nerve, inferior branch"
				action="adducts the eyeball"
				/>
			<element name="Superior rectus muscle" type="cranial" link="http://en.wikipedia.org/wiki/Superior_rectus_muscle"
				nerves="oculomotor nerve, superior branch"
				action="elevates, adducts, and rotates medially the eye"
				/>
		</element>

		<element name="Oblique">
			<element name="Inferior oblique muscle" type="cranial" link="http://en.wikipedia.org/wiki/Inferior_oblique_muscle"
				nerves="oculomotor nerve, inferior branch"
				action="extorsion, elevation, abduction"
				/>
			<element name="Superior oblique muscle" type="cranial" link="http://en.wikipedia.org/wiki/Superior_oblique_muscle"
				nerves="trochlear nerve"
				action="primary: intorsion, secondary:abduct (laterally rotate) and depress the eyeball"
				/>
		</element>

	</division>

	<division category="MUSCLES - HEAD" name="Intraocular muscles">

		<element name="Ciliary muscle">
			<element name="Ciliary muscle, oculomotor" type="cranial" link="http://en.wikipedia.org/wiki/Ciliary_muscle"
				nerves="ciliary nerves, short, parasympathetic fibers"
				action="accommodation"
				/>
			<element name="Ciliary muscle, carotid" type="cranial" link="http://en.wikipedia.org/wiki/Ciliary_muscle"
				nerves="ciliary nerves, sympathetic fibers"
				action="accommodation"
				/>
		</element>

		<element name="Iris dilator muscle" type="cranial" link="http://en.wikipedia.org/wiki/Iris_dilator_muscle"
			nerves="ciliary nerves, sympathetic fibers"
			action="pupil dilation"
			/>

		<element name="Iris sphincter muscle" type="cranial" link="http://en.wikipedia.org/wiki/Iris_sphincter_muscle"
			nerves="ciliary nerves, short, parasympathetic fibers"
			action="constricts pupil"
			/>

		<element name="Orbitalis muscle" type="cranial" link="http://en.wikipedia.org/wiki/Orbitalis_muscle"
			nerves="ciliary nerves, sympathetic fibers"
			action="protrusion of eyeball"
			/>

	</division>

	<division category="MUSCLES - HEAD" name="Ear" group="facial, auricular muscles; muscles of auditory ossicles">

		<element name="Auricular muscles">
			<element name="Anterior auricular muscle" type="cranial" link="http://en.wikipedia.org/wiki/Anterior_auricular_muscle"
				nerves="facial nerve, temporal branch"
				action="pulls ear forward"
				/>
			<element name="Posterior auricular muscle" type="cranial" link="http://en.wikipedia.org/wiki/Posterior_auricular_muscle"
				nerves="auricular nerve, posterior"
				action="pulls ear backward"
				/>
			<element name="Superior auricular muscle" type="cranial" link="http://en.wikipedia.org/wiki/Superior_auricular_muscle"
				nerves="auricular nerve, posterior"
				action="pulls ear upward"
				/>
			<element name="Oblique muscle of auricle" type="cranial" link="http://en.wikipedia.org/wiki/Oblique_muscle_of_auricle"
				nerves="auricular nerve, posterior, auricular branch"
				action="movement of the ears"
				/>
		</element>

		<element name="Temporoparietalis muscle" type="cranial" link="http://en.wikipedia.org/wiki/Temporoparietalis_muscle"
			nerves="facial nerve, temporal branch"
			action="assist the frontal belly in drawing the epicranial aponeurosis towards the front of the cranium"
			/>

		<element name="Stapedius muscle" type="cranial" link="http://en.wikipedia.org/wiki/Stapedius"
			nerves="stapedius nerve"
			action="control the amplitude of sound waves to the inner ear"
			/>

		<element name="Tensor tympani muscle" type="cranial" link="http://en.wikipedia.org/wiki/Tensor_tympani"
			nerves="nerve to tensor tympani"
			action="tensing the tympanic membrane"
			/>

	</division>

	<division category="MUSCLES - HEAD" name="Nose" group="Facial muscles">

		<element name="Procerus muscle" type="cranial" link="http://en.wikipedia.org/wiki/Procerus_muscle"
			nerves="facial nerve, buccal branch"
			action="draws down the medial angle of the eyebrow giving expressions of frowning"
			/>

		<element name="Nasalis muscle" type="cranial" link="http://en.wikipedia.org/wiki/Nasalis_muscle"
			nerves="facial nerve, buccal branch"
			action="compresses bridge, depresses tip of nose, elevates corners of nostrils, dilation of nostrils"
			/>

		<element name="Depressor septi nasi muscle" type="cranial" link="http://en.wikipedia.org/wiki/Depressor_septi_nasi_muscle"
			nerves="facial nerve, buccal branch"
			action="depression of nasal septum"
			/>

		<element name="Levator labii superioris alaeque nasi muscle" type="cranial" link="http://en.wikipedia.org/wiki/Levator_labii_superioris_alaeque_nasi_muscle"
			nerves="facial nerve, buccal branch"
			action="dilates the nostril; elevates the upper lip and wing of the nose"
			/>

	</division>

	<division category="MUSCLES - HEAD" name="Mouth" group="Facial muscles">

		<element name="Levator anguli oris" type="cranial" link="http://en.wikipedia.org/wiki/Levator_anguli_oris"
			nerves="facial nerve, buccal branch"
			action="smile (elevates angle of mouth)"
			/>

		<element name="Depressor anguli oris muscle" type="cranial" link="http://en.wikipedia.org/wiki/Depressor_anguli_oris_muscle"
			nerves="facial nerve, marginal mandibular branch"
			action="depresses angle of mouth"
			/>

		<element name="Transverse muscle of the chin" type="cranial" link="http://en.wikipedia.org/wiki/Transverse_muscle_of_the_chin"
			nerves="facial nerve, marginal mandibular branch"
			action="assist with pulling the lower lip downward and help to tighten the skin over the chin"
			/>

		<element name="Levator labii superioris muscle" type="cranial" link="http://en.wikipedia.org/wiki/Levator_labii_superioris"
			nerves="facial nerve, zygomatic branch"
			action="elevates the upper lip"
			/>

		<element name="Depressor labii inferioris muscle" type="cranial" link="http://en.wikipedia.org/wiki/Depressor_labii_inferioris_muscle"
			nerves="facial nerve, marginal mandibular branch"
			action="depress the lower lip"
			/>

		<element name="Zygomatic muscles">
			<element name="Zygomaticus major muscle">
				<element name="Zygomaticus major muscle, proper" type="cranial" link="http://en.wikipedia.org/wiki/Zygomaticus_major_muscle"
					nerves="facial nerve, zygomatic branch"
					action="draws angle of mouth upward and laterally"
					/>
				<element name="Zygomaticus major muscle, buccal" type="cranial" link="http://en.wikipedia.org/wiki/Zygomaticus_major_muscle"
					nerves="facial nerve, buccal branch"
					action="draws angle of mouth upward and laterally"
					/>
			</element>
			<element name="Zygomaticus minor muscle" type="cranial" link="http://en.wikipedia.org/wiki/Zygomaticus_minor_muscle"
				nerves="facial nerve, buccal branch"
				action="elevates upper lip"
				/>
		</element>

		<element name="Mentalis muscle" type="cranial" link="http://en.wikipedia.org/wiki/Mentalis"
			nerves="facial nerve, marginal mandibular branch"
			action="elevates and wrinkles skin of chin, protrudes lower lip"
			/>

		<element name="Buccinator muscle" type="cranial" link="http://en.wikipedia.org/wiki/Buccinator_muscle"
			nerves="facial nerve, buccal branch"
			action="compress the cheeks against the teeth (blowing), mastication"
			/>

		<element name="Orbicularis oris muscle" type="cranial" link="http://en.wikipedia.org/wiki/Orbicularis_oris_muscle"
			nerves="facial nerve, buccal branch"
			action="pucker the lips"
			/>

		<element name="Risorius" type="cranial" link="http://en.wikipedia.org/wiki/Risorius"
			nerves="facial nerve, buccal branch"
			action="draw back angle of mouth"
			/>

	</division>

	<division category="MUSCLES - HEAD" name="Mastication" group="Muscles of mastication">

		<element name="Pterygoid muscles">
			<element name="Lateral pterygoid muscle" type="cranial" link="http://en.wikipedia.org/wiki/Lateral_pterygoid_muscle"
				nerves="pterygoid nerve, lateral"
				action="depresses mandible"
				/>
			<element name="Medial pterygoid muscle" type="cranial" link="http://en.wikipedia.org/wiki/Medial_pterygoid_muscle"
				nerves="pterygoid nerve, medial"
				action="elevates mandible, closes jaw, helps lateral pterygoids in moving the jaw from side to side"
				/>
		</element>

		<element name="Masseter muscle" type="cranial" link="http://en.wikipedia.org/wiki/Masseter_muscle"
			nerves="masseteric nerve"
			action="elevation (as in closing of the mouth) and retraction of mandible"
			/>

		<element name="Temporal muscle" type="cranial" link="http://en.wikipedia.org/wiki/Temporalis_muscle"
			nerves="temporal nerves, deep"
			action="elevation and retraction of mandible"
			/>

	</division>

	<division category="MUSCLES - HEAD" name="Tongue" group="Muscles of tongue; Muscles of soft palate and fauces">

		<element name="Extrinsic">
			<element name="Genioglossus muscle" type="cranial" link="http://en.wikipedia.org/wiki/Genioglossus"
				nerves="hypoglossal nerve"
				action="complex - inferior fibers protrude the tongue, middle fibers depress the tongue, and its superior fibers draw the tip back and down"
				/>
			<element name="Hyoglossus muscle" type="cranial" link="http://en.wikipedia.org/wiki/Hyoglossus"
				nerves="hypoglossal nerve"
				action="depresses tongue"
				/>
			<element name="Styloglossus muscle" type="cranial" link="http://en.wikipedia.org/wiki/Styloglossus"
				nerves="hypoglossal nerve"
				action="elevates and retracts tongue"
				/>
			<element name="Chondroglossus muscle" type="cranial" link="http://en.wikipedia.org/wiki/Chondroglossus"
				nerves="hypoglossal nerve"
				action="depresses tongue"
				/>
			<element name="Palatoglossus muscle" type="cranial" link="http://en.wikipedia.org/wiki/Palatoglossus_muscle"
				nerves="vagus nerve, pharyngeal plexus"
				action="raising the back part of the tongue"
				/>
		</element>

		<element name="Intrinsic">
			<element name="Inferior longitudinal muscle of tongue" type="cranial" link="http://en.wikipedia.org/wiki/Inferior_longitudinal_muscle"
				nerves="hypoglossal nerve"
				action="shortens, retracts, pulls tip downward"
				/>
			<element name="Superior longitudinal muscle of tongue" type="cranial" link="http://en.wikipedia.org/wiki/Superior_longitudinal_muscle"
				nerves="hypoglossal nerve"
				action="shortens, turns tip upward, turns lateral margins upward"
				/>
			<element name="Transverse muscle of tongue" type="cranial" link="http://en.wikipedia.org/wiki/Transversus_muscle"
				nerves="hypoglossal nerve"
				action="narrows and elongates"
				/>
			<element name="Vertical muscle of tongue" type="cranial" link="http://en.wikipedia.org/wiki/Verticalis_muscle"
				nerves="hypoglossal nerve"
				action="flattens"
				/>
		</element>

	</division>

	<division category="MUSCLES - HEAD" name="Soft palate" group="Muscles of soft palate and fauces">

		<element name="Levator veli palatini muscle" type="cranial" link="http://en.wikipedia.org/wiki/Levator_veli_palatini"
			nerves="vagus nerve, pharyngeal branch"
			action="elevates soft palate"
			/>

		<element name="Musculus uvulae muscle" type="cranial" link="http://en.wikipedia.org/wiki/Musculus_uvulae"
			nerves="palatine nerves, lesser, parasympathetic fibers"
			action="elevates and retracts the uvula, thickening the central part of the soft palate"
			/>

		<element name="Palatopharyngeus muscle" type="cranial" link="http://en.wikipedia.org/wiki/Palatopharyngeus_muscle"
			nerves="vagus nerve, pharyngeal branch"
			action="pulls pharynx and larynx"
			/>

		<element name="Tensor veli palatini muscle" type="cranial" link="http://en.wikipedia.org/wiki/Tensor_veli_palatini_muscle"
			nerves="nerve to tensor veli palatini"
			action="tension of the soft palate"
			/>

	</division>

	<division category="MUSCLES - HEAD" name="Pharynx" group="Muscles of pharynx">

		<element name="Pharyngeal constrictor">
			<element name="Inferior pharyngeal constrictor muscle" type="cranial" link="http://en.wikipedia.org/wiki/Inferior_pharyngeal_constrictor_muscle"
				nerves="laryngeal nerve, external"
				action="swallowing"
				/>
			<element name="Middle pharyngeal constrictor muscle" type="cranial" link="http://en.wikipedia.org/wiki/Middle_pharyngeal_constrictor_muscle"
				nerves="vagus nerve, pharyngeal plexus"
				action="swallowing"
				/>
			<element name="Superior pharyngeal constrictor muscle" type="cranial" link="http://en.wikipedia.org/wiki/Superior_pharyngeal_constrictor_muscle"
				nerves="vagus nerve, pharyngeal plexus"
				action="swallowing"
				/>
		</element>

		<element name="Salpingopharyngeus muscle" type="cranial" link="http://en.wikipedia.org/wiki/Salpingopharyngeus_muscle"
			nerves="vagus nerve, pharyngeal plexus"
			action="raise the nasopharynx"
			/>

		<element name="Stylopharyngeus muscle" type="cranial" link="http://en.wikipedia.org/wiki/Stylopharyngeus_muscle"
			nerves="glossopharyngeal nerve, stylopharyngeal branch"
			action="elevate the larynx, elevate the pharynx, swallowing"
			/>

		<element name="Pharyngeal tonsils" type="gland" link="http://www.wisegeekhealth.com/what-is-a-pharyngeal-tonsil.htm"
			nerves="pharyngeal nerve"
			action="enhance the capabilities of the lymphatic system against foreign agents that enter through nose"
			/>

	</division>

	<division category="MUSCLES - HEAD" name="Larynx" group="Muscles of larynx, intrinsic">

		<element name="Cricothyroid muscle" type="cranial" link="http://en.wikipedia.org/wiki/Cricothyroid_muscle"
			nerves="laryngeal nerve, external"
			action="tension and elongation of the vocal folds (has minor adductory effect)"
			/>

		<element name="Cricoarytenoid muscle">
			<element name="Lateral cricoarytenoid muscle" type="cranial" link="http://en.wikipedia.org/wiki/Lateral_cricoarytenoid_muscle"
				nerves="inferior laryngeal nerve"
				action="adduct and medially rotate the cartilage, pulling the vocal ligaments towards the midline and backwards and so closing off the rima glottidis"
				/>
			<element name="Posterior cricoarytenoid muscle" type="cranial" link="http://en.wikipedia.org/wiki/Posterior_cricoarytenoid_muscle"
				nerves="inferior laryngeal nerve"
				action="abducts and laterally rotates the cartilage, pulling the vocal ligaments away from the midline and forward and so opening the rima glottidis"
				/>
		</element>

		<element name="Oblique arytenoid muscle" type="cranial" link="http://en.wikipedia.org/wiki/Oblique_arytenoid"
			nerves="inferior laryngeal nerve"
			action="sphincter of the laryngeal inlet"
			/>

		<element name="Thyroarytenoid muscle" type="cranial" link="http://en.wikipedia.org/wiki/Thyroarytenoid_muscle"
			nerves="inferior laryngeal nerve"
			action="thickens the vocal folds and decreases length; also helps to adduct the vocal folds during speech"
			/>

		<element name="Arytenoid muscle" type="cranial" synonym="Transverse arytenoid" link="http://en.wikipedia.org/wiki/Arytenoid_muscle"
			nerves="laryngeal nerve, external"
			action="approximate the arytenoid cartilages (close rima glottidis)"
			/>

		<element name="Aryepiglottic muscle" type="cranial" link="http://en.wikipedia.org/wiki/Aryepiglottic_muscle"
			nerves="recurrent laryngeal nerve"
			action="close the inlet"
			/>

		<element name="Levator muscle of thyroid gland" type="cranial" link="http://en.wikipedia.org/wiki/Levator_muscle_of_thyroid_gland"
			nerves="laryngeal nerve, external"
			action="elevates the thyroid gland"
			/>

		<element name="Thyroepiglottic muscle" type="cranial" link="http://en.wikipedia.org/wiki/Thyroepiglottic_muscle"
			nerves="inferior laryngeal nerve"
			action="closes the inlet to the larynx"
			/>

	</division>

</pns>
